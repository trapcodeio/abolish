{
  "name": "abolish",
  "version": "4.11.1",
  "description": "A javascript object validator.",
  "main": "index.js",
  "module": "index.esm.js",
  "unkpg": "browser.min.js",
  "jsdelivr": "browser.min.js",
  "types": "index.d.ts",
  "repository": "https://github.com/trapcodeio/abolish",
  "author": "trapcodeio",
  "homepage": "https://abolish.trapcode.io",
  "license": "MIT",
  "private": false,
  "browser": {
    "lodash": "lodash-es"
  },
  "scripts": {
    "build": "npx tsc && npm run bundle",
    "bundle": "node bundle.js",
    "prepublishOnly": "npm run test",
    "watch": "npx tsc --watch",
    "play": "npx ts-node-dev --respawn tests/play.ts",
    "test-browser": "npx vite --host",
    "test": "npm run build && node JapaFile.js",
    "test-dev": "npx nodemon JapaFile.js --watch '*/**' --ext 'ts'"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.23",
    "esbuild": "^0.14.27",
    "japa": "^4.0.0",
    "joi": "^17.6.0",
    "lodash-es": "^4.17.21",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2",
    "yup": "^0.32.11"
  },
  "keywords": [
    "abolish",
    "object-validator-pro",
    "object-validation",
    "object-validator",
    "async-object-validator"
  ]
}
